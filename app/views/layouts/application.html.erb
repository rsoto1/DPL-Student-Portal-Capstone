<!DOCTYPE html>
<html>
  <head>
    <title>Capstone</title>
    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="wrapper"> <!-- body wrapper -->
      <%= render '/layouts/nav' %>
      <div id="page-wrapper"> <!-- Main content wrapper -->
        <%= render '/layouts/breadcrumbs' %>
        <div class="container-fluid">

          <%= yield %>
      </div>
    </div>
      <div class="wrapper">
        <div class="push"></div>
      </div>
      <div class="footer">
        <p class="Copyright">&copy; DevPoint Labs 2014</p>
        <p class="Copyright"><%= link_to 'About', about_path %> | <%= link_to 'Contact', contact_path %></p>
      </div>

  </body>
  
</html>


    
